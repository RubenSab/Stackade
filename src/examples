# count from 0 to 10

0
{
    (dup 10 <=)
    (dup print " " print (1 +) self) # true
    ("done!" print)                  # false
}


# identity matrix

"printMatrix" [

    "n" swap :num
    "i" 0 :num
    "j" 0 :num

    {
        (i n <)
        (
            {
                (j n <)
                ({(i j ==)(1)(0)} print j ++ self)
                (j 0 =)
            }

            "\n" print
            i ++
            self
        )
    }
] :fblock

10 printMatrix