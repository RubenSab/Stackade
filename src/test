"printMatrix" [

    "size" swap :num
    "y" 0 :num
    "x" 0 :num

    {
        (y size <)
        (
            {
                (x size <)
                ({(y x ==)(1)(0)} print x ++ self)
                (x 0 =)
            }

            "\n" print
            y ++
            self
        )
    }
] :fblock

5 printMatrix