"src/testing/printUtils" run

"++" (
    dup dup refName swap 1 + :num refName :raise
) :seq

"printMatrix" (

    "n" swap :num
    "i" 0 :num
    "j" 0 :num

    {
        (i n <)
        (
            {
                (j n <)
                ({(i j ==)(1)(0)} prints j ++ self)
                (j 0 =)
            }

            nline
            i ++
            self
        )
    }
) :seq

10 printMatrix
nline
5 printMatrix

debug