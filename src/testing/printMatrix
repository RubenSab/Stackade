"++" (
    dup dup refName swap 1 + :num refName :raise
) :seq

"printMatrix" (

    "n" swap :num
    "i" 0 :num
    "j" 0 :num

    {
        (i n <)
        (
            {
                (j n <)
                ({(i j ==)(1)(0)} print " " print j dup 1 + = self)
                (j 0 =)
            }

            "\n" print
            i dup 1 + =
            self
        )
    }
) :seq